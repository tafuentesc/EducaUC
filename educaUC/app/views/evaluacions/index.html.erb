<h1>Listing evaluacions</h1>

<table>
  <tr>
    <th>Nombre sala</th>
    <th>Fecha de evaluacion</th>
    <th>Encargado</th>
    <th>Horario inicial</th>
    <th>Horario final</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @evaluacions.each do |evaluacion| %>
  <tr>
    <td><%= evaluacion.nombre_sala %></td>
    <td><%= evaluacion.fecha_de_evaluacion %></td>
    <td><%= evaluacion.encargado %></td>
    <td><%= evaluacion.horario_inicial %></td>
    <td><%= evaluacion.horario_final %></td>
    <td><%= link_to 'Show', evaluacion %></td>
    <td><%= link_to 'Edit', edit_evaluacion_path(evaluacion) %></td>
    <td><%= link_to 'Destroy', evaluacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Evaluacion', new_evaluacion_path %>
